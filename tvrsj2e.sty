%2000年問題
\newcount\year@cnt
\def\YEAR#1{\def\@tempa{#1}
 \ifx \@tempa\empty \year@cnt=2000\gdef\@YEAR{2000}\else
  \year@cnt=#1\gdef\@YEAR{#1}\fi
 \ifnum \year@cnt <100\relax \gdef\@YEAR{20#1} \else 
  \advance\year@cnt by-100\relax \fi
 \gdef\h@YEAR{\the\year@cnt}
}


